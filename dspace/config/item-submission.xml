        <name-map collection-handle="123456789/29" submission-name="Goias"/>
        <name-map collection-handle="123456789/7" submission-name="Maranhao"/>
        <name-map collection-handle="123456789/26" submission-name="MinasGerais"/>
        <name-map collection-handle="123456789/31" submission-name="MatoGrossoSul"/>
        <name-map collection-handle="123456789/30" submission-name="MatoGrosso"/>
        <name-map collection-handle="123456789/18" submission-name="Para"/>
        <name-map collection-handle="123456789/10" submission-name="Paraiba"/>
        <name-map collection-handle="123456789/11" submission-name="Pernambuco"/>
